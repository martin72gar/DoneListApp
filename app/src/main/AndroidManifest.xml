<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.martinsiregar.donelistapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DoneListApp"
        android:usesCleartextTraffic="true">
        <activity android:name=".EditDonelistActivity"></activity>
        <activity android:name=".EditActivity" />
        <activity android:name=".InsertActivity" />
        <activity android:name=".KontakMainActivity"></activity>
        <activity android:name=".AddDoneListActivity" />
        <activity android:name=".DoneListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".SignupActivity" />
        <activity android:name=".MainActivity"></activity>
    </application>

</manifest>